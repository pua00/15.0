<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_pua_cartridge_type_of_cartridge_tree" model="ir.ui.view">
        <field name="name">pua_cartridge_type_of_cartridge.tree</field>
        <field name="model">pua_cartridge.type_of_cartridge</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_pua_cartridge_type_of_cartridge_form" model="ir.ui.view">
        <field name="name">pua_cartridge_type_of_cartridge.form</field>
        <field name="model">pua_cartridge.type_of_cartridge</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="pua_cartridge_type_of_cartridge_action" model="ir.actions.act_window">
        <field name="name">Type of cartridge</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">pua_cartridge.type_of_cartridge</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="pua_cartridge_stype_of_cartridge_menu"
              action="pua_cartridge_type_of_cartridge_action"
              parent="pua_cartridge_catalog_menu"
              name="Type of cartridge"
              sequence="300"/>

</odoo>



